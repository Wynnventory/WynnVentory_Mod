{
  "schemaVersion": 1,
  "id": "wynnventory",
  "version": "${mod_version}",
  "name": "wynnventory",
  "description": "Real-time insights and historical data from the Wynncraft Trade Market.",
  "authors": [
    "Aruloci",
    "SiropBVST"
  ],
  "contact": {
    "homepage": "https://www.wynnventory.com/",
    "sources": "https://github.com/Aruloci/Wynnventory"
  },
  "license": "MIT",
  "icon": "assets/wynnventory/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.wynnventory.fabric.WynnventoryModFabric"
    ]
  },
  "mixins": [
    "wynnventory.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=$fabric_loader_version",
    "minecraft": "~$minecraft_version",
    "java": ">=21",
    "wynntils": ">=$wynntils_version"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/b6ATfrePuR",
        "modmenu.website": "https://www.wynnventory.com/"
      },
      "update_checker": false
    }
  }
}
